            status:'',       //状态
            statuss:'',          //状态下拉列表数据
            editDate:'',       //修改日期
           }
        }
      },
      methods: {
        handleQuery:function(){       //新建城市保存

        },
        handleQueryEdit:function(){           //编辑城市保存

        }
      },
      mounted:function(){
        axios.post('/user', {
            firstName: 'Fred',
            lastName: 'Flintstone'
        })
        .then(function (response) {
