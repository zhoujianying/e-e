<!DOCTYPE html>
<html lang="zh-CN">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title> 
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
    <link rel="stylesheet" href="./style/public.css">
    <style>
        td{border:solid #dcdfe6; border-width:0px 1px 1px 0px; padding:10px;}
        table{border:solid #dcdfe6; border-width:1px 0px 0px 1px; border-radius: 4px}
    </style>
  </head>
  <body class="body">
    <h3 style="font-weight: 400">批量创建</h3>
    <div id='app'>
      <el-card class="box-card">
        <div slot="header" class="clearfix">
            <span>主数据批量创建</span>
        </div>
        <div>
            <el-row :gutter="20">
                <el-col :span="8">
                    <div class="grid-content bg-purple">
                        <span class="laber">城市:</span>
                        <div class="inputBox">
                          <el-select v-model="city" placeholder="请选择">
                              <el-option
                                  v-for="item in citys"
                                  :key="item.value"
                                  :label="item.label"
                                  :value="item.value">
                              </el-option>
                          </el-select>
                        </div>                   
                    </div>
                </el-col>
                <el-col :span="8">
                    <div class="grid-content bg-purple">
                        <span class="laber">县区:</span>
                        <div class="inputBox">
                          <el-select v-model="conty" placeholder="请选择">
                              <el-option
                                  v-for="item in contys"
                                  :key="item.value"
                                  :label="item.label"
                                  :value="item.value">
                              </el-option>
                          </el-select>
                        </div>                   
                    </div>
                </el-col>
                <el-col :span="8">
                    <div class="grid-content bg-purple">
                        <span class="laber">街道/乡镇:</span>
                        <div class="inputBox">
                              <el-select v-model="street" placeholder="请选择">
                                  <el-option
                                      v-for="item in streets"
                                      :key="item.value"
                                      :label="item.label"
                                      :value="item.value">
                                  </el-option>
                              </el-select>
                        </div>                   
                    </div>
                </el-col>
                <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">小区/行政村:</span>
                            <div class="inputBox">
                              <el-select v-model="quarter" placeholder="请选择">
                                  <el-option
                                      v-for="item in quarters"
                                      :key="item.value"
                                      :label="item.label"
                                      :value="item.value">
                                  </el-option>
                              </el-select>
                            </div>                   
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">楼栋:</span>
                            <div class="inputBox">
                              <el-select v-model="building" placeholder="请选择">
                                  <el-option
                                      v-for="item in buildings"
                                      :key="item.value"
                                      :label="item.label"
                                      :value="item.value">
                                  </el-option>
                              </el-select>
                            </div>                   
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">起始单元:</span>
                            <div class="inputBox">
                                <el-input v-model="startUnit" placeholder="请输入内容"></el-input>
                            </div>                   
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">单元数:</span>
                            <div class="inputBox">
                                <el-input v-model="unitNum" placeholder="请输入内容"></el-input>
                            </div>           
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">起始房间号:</span>
                            <div class="inputBox">
                                <el-input v-model="startRoomNum" placeholder="请输入内容"></el-input>
                            </div>           
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">户数:</span>
                            <div class="inputBox">
                                <el-input v-model="households" placeholder="请输入内容"></el-input>
                            </div>           
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">楼层数:</span>
                            <div class="inputBox">
                                <el-input v-model="floorsNum" placeholder="请输入内容"></el-input>
                            </div>           
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">房产类型:</span>
                            <div class="inputBox">
                                <el-select v-model="propertyType" placeholder="请选择">
                                    <el-option
                                        v-for="item in propertyTypes"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                    </el-option>
                                </el-select>
                            </div>           
                        </div>
                    </el-col>
                    <el-col :span="8">
                        <div class="grid-content bg-purple">
                            <span class="laber">房产状态:</span>
                            <div class="inputBox">
                                <el-select v-model="propertyStatu" placeholder="请选择">
                                    <el-option
                                        v-for="item in propertyStatus"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                    </el-option>
                                </el-select>
                            </div>           
                        </div>
                    </el-col>
                    <el-col :span="16">
                        <div class="grid-content bg-purple">
                            <span class="laber">是否导入接驳记录:</span>
                            <div class="inputBox" style="margin-top: 10px">
                                <template>
                                    <el-radio v-model="radio" label="1">是</el-radio>
                                    <el-radio v-model="radio" label="2">否</el-radio>
                                </template>
                            </div>           
                        </div>
                    </el-col>
              </el-row>       
          </div>
      </el-card> 
      <el-card class="box-card">
            <div slot="header" class="clearfix">
                <span>设备信息</span>
            </div>
            <div>
                <!-- <template>
                    <el-table :data="tableData"  border  style="width: 100%">                       
                        <el-table-column prop="province" label="设备"></el-table-column>
                        <el-table-column prop="city" label="品牌"></el-table-column>
                        <el-table-column prop="city" label="状态"></el-table-column>
                        <el-table-column type="selection" label='全选'>全选</el-table-column>
                    </el-table>
                </template> -->
                <table border="1" width='100%' cellpadding="0" cellspacing="0">
                    <thead border='0'>
                        <td>设备</td>
                        <td>品牌</td>
                        <td>状态</td>
                        <td><el-checkbox v-model="checked">全选</el-checkbox></td>  
                    </thead>
                    <tbody>
                        <tr>
                            <td>燃气管道</td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template> 
                            </td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td><el-checkbox v-model="checked"></el-checkbox></td>
                        </tr>
                        <tr>
                            <td>燃气表</td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td><el-checkbox v-model="checked"></el-checkbox></td>
                        </tr>
                        <tr>
                            <td>灶具</td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td><el-checkbox v-model="checked"></el-checkbox></td>
                        </tr>
                        <tr>
                            <td>热水器</td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td><el-checkbox v-model="checked"></el-checkbox></td>
                        </tr>
                        <tr>
                            <td>抽油烟机</td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td>
                                <template>
                                    <el-select v-model="value" placeholder="请选择">
                                        <el-option
                                        v-for="item in options"
                                        :key="item.value"
                                        :label="item.label"
                                        :value="item.value">
                                        </el-option>
                                    </el-select>
                                </template>
                            </td>
                            <td><el-checkbox v-model="checked"></el-checkbox></td>
                        </tr>
                    </tbody>
                </table>
            </div>
      </el-card>  
      <div class="grid-content bg-purple" style="display:flex; justify-content: flex-end; margin-top: 20px">
        <el-button type="primary" @click='handleCreate'>创建</el-button>
      </div>
    </div>
   
  </body>
  <script src="https://unpkg.com/vue/dist/vue.js"></script>
  <script src="https://unpkg.com/element-ui/lib/index.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script>
    new Vue({
      el: '#app',
      data: function() {
        return { 
          radio: '1',
          city:'',            //城市
          citys:[],         //城市下拉列表数据
          conty:'',          //县区
          contys:[],         //县区下拉列表数据
          street:'',        //街道
          streets:[],         //街道下拉列表数据
          quarter:'',    //小区、行政村
          quarters:[],           //小区、行政村下拉列表数据
          building:'',   //楼栋
          buildings:[],   //楼栋下拉列表数据
          startUnit:'',     //起始单元
          unitNum:'',        //单元数
          startRoomNum:'',         //其实房间号
          households:'',         //户数
          floorsNum:'',         //楼层数
          propertyType:'',      //房产类型
          propertyTypes:[],        //房产类型下拉列表数据
          propertyStatu:'',         //房产状态
          propertyStatus:[],             //房产状态下拉列表数据
        }
      },
      methods: {
        handleCreate() {
            this.$confirm('本次批导创建成功90条，失败0条，请确认！', '数据批导确认', {
            confirmButtonText: '确定',
            cancelButtonText: '取消',
            type: 'warning'
            }).then(() => {
            // this.$message({
            //     type: 'success',
            //     message: '删除成功!'
            // });
            }).catch(() => {
            // this.$message({
            //     type: 'info',
            //     message: '已取消删除'
            // });          
            });
        }
      },
      mounted:function(){
        axios.post('/user', {
            firstName: 'Fred',
            lastName: 'Flintstone'
        })
        .then(function (response) {
            console.log(response);
        })
        .catch(function (error) {
            console.log(error);
        });
      }
    })
  </script>
</html>