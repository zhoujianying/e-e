                                    v-for="item in oldWatch.replaceStates"
                                    :key="item.value"
                                    :label="item.label"
                                    :value="item.value">
                                </el-option>
                            </el-select>
                        </div>                   
                    </div>
                </el-col>
            </el-row>       
        </div>
