  <script src="https://unpkg.com/vue/dist/vue.js"></script>
  <script src="https://unpkg.com/element-ui/lib/index.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script>
    new Vue({
      el: '#app',
      data: function() {
        return { 
          accountIsshow:false,       //账户模块显示隐藏
          oldWatch:{
              houseNum:'',        //房产编号
              adress:'',              //地址
              replaceState:'',     //置换状态
              replaceStates:[],     //置换状态下拉列表数据
              needleNum:'',         //表针编号
              materialNum:'',     //物料号
              model:'',       //型号
              buildDate:'',     //生产日期
              gasType:'',      //用气类型
              tableSteelNumbe:'', //表钢号
              installDate:'',      //安装日期
              installPosition:'',    //安装位置
              installPositions:[],    //安装位置下拉列表数据
              identification:'',    //户内外标识
              identifications:[],   //户内外标识下拉列表数据
              inatallState:'',      //安装状态
              inatallStates:[],    //安装状态下拉列表状态
              warrantyStartDate:'',    //保修开始日期
              warrantyEndDate:'',     //保修结束日期
              baseTableRead:'',       //基表读数
              liquidCrystalRead:'',    //液晶读数
              cumuConsu:'',        //  累计用气量
              changeTableCause:'',          //换表原因
              changeTableCauses:[],           //换表原因下拉列表数据
              balance:'', //余额
              refundsBalance:'',     //退款余额
           },
           newWatch:{
              tableSteelNum:'',       //表钢号
              materialNum:'',      //物料号
              model:'',      //型号
              buildDate:'',        //生产日期
              gasType:'',       //用气类型
              installState:'',     //安装状态
              installStates:[],          //安装状态下拉列表数据
              installDate:'',    //安装日期
              installPosition:'',        //安装位置
              installPositions:[],       //安装位置下拉列表数据
              identification:'',          //户内外标识
              identifications:[],           //户内外标识下拉列表数据
              warrantyStartDate:'',    //保修开始日期
              warrantyEndDate:'',          //保修结束日期
              readingTableUnit:'',        //读表单元
              price:'',         //价格
              readingTableUnits:[],           //读表单元下拉列表数据
              baseTtableRead:'',       //基表读数
              amountCollected:'',            //收款金额
              staAccumuGasCon:'', //阶梯累计用气量
           },
        }
      },
      methods: {
        handlePreservation:function(){
            this.$message('表计设备换表成功！');
        },
      },
      mounted:function(){
        axios.post('/user', {
            firstName: 'Fred',
            lastName: 'Flintstone'
        })
        .then(function (response) {
            console.log(response);
        })
        .catch(function (error) {
            console.log(error);
        });
      }
    })
  </script>
