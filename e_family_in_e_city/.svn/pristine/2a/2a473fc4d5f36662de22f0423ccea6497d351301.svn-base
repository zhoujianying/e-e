                                        :label="item.label"
                                        :value="item.value">
                                    </el-option>
                                </el-select>
                          </div>                   
                      </div>
                  </el-col>
                  <el-col :span="8">
                      <div class="grid-content bg-purple">
                          <span class="laber">证件类型:</span>
