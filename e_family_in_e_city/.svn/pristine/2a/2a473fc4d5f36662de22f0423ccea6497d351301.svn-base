              <el-row :gutter="20">
                  <el-col :span="8">
                      <div class="grid-content bg-purple">
                          <span class="laber">家庭类型:</span>
                          <div class="inputBox">
                            <el-select v-model="userInfor.familyType" placeholder="请选择">
                                <el-option
                                    v-for="item in userInfor.familyTypes"
                                    :key="item.value"
                                    :label="item.label"
                                    :value="item.value">
                                </el-option>
                            </el-select>
                          </div>                   
                      </div>
                  </el-col>
                  <el-col :span="8">
                      <div class="grid-content bg-purple">
                          <span class="laber">家庭标签:</span>
                          <div class="inputBox">
                            <el-select v-model="userInfor.familyLaber" placeholder="请选择">
                                <el-option
                                    v-for="item in userInfor.familyLabers"
                                    :key="item.value"
                                    :label="item.label"
                                    :value="item.value">
                                </el-option>
                            </el-select>
                          </div>                   
                      </div>
                  </el-col>
