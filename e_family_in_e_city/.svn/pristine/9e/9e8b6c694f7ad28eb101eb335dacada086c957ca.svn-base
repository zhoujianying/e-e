      methods: {
           //   第一部分（客户信息）
        handleSlideDown:function(){     // 点击其他成员下拉按钮表单显示
            this.otherFamilyDown=false;  
            this.otherFamilyUp=true;  
            this.otherInforIsshow=true;
        },
        handleSlideUp:function(){     // 点击其他成员收起按钮表单隐藏
            this.otherFamilyDown=true;  
            this.otherFamilyUp=false;
            this.otherInforIsshow=false;
        },
        handleDialogEditCanel:function(){    //点击编辑弹出框取消按钮
            this.dialogTableVisibleUserEdit = false
        },
        handleDialogEditSure:function(){    //点击编辑弹出框确定按钮
            this.dialogTableVisibleUserEdit = false
        },
        handleDialogResgiter:function(){    //点击注册用户信息弹出框关闭按钮
            this.dialogTableVisibleUserRegister = false
        },
        // 第二部分（房产信息）
        handleDialogCance:function(){    //点击修改地址弹出窗关闭按钮
            this.dialogFormVisible = false;
        },
        handleDialogDeposit:function(){    //点击修改地址弹出窗保存按钮
            this.dialogFormVisible = false;
        },
        handleDialogCanceEdit:function(){    //点击修改地址弹出窗关闭按钮
            this.dialogFormVisibleEdit = false;
        },
        handleDialogDepositEdit:function(){    //点击修改地址弹出窗保存按钮
            this.dialogFormVisibleEdit = false;
        },
        //   第三部分(表针信息)
        handlePartsDown:function(){     //点击配件内容展开
            this.watchNeedle.tableIsshow = true;
            this.watchNeedle.partsDown = false;
            this.watchNeedle.partsUp = true;
        },
        handlePartsUp:function(){     //点击配件内容收回
            this.watchNeedle.tableIsshow = false;
            this.watchNeedle.partsDown = true;
            this.watchNeedle.partsUp = false;
        },
        handleEditWatchNeedle:function(){      // 点击编辑表计弹窗显示隐藏
            this.watchNeedle.editWatchNeedle = true; 
        },
        handleWatchNeedleEditCanle:function(){    //点击编辑表针弹窗取消按钮
            this.watchNeedle.editWatchNeedle = false;
        },
        handleWatchNeedleEditSure:function(){    //点击编辑表针弹窗确定按钮
            this.watchNeedle.editWatchNeedle = false;
        },
        handleChangerWatchNeedle:function(){   //点击更改物料弹窗显示隐藏
            this.watchNeedle.changerWatchNeedle = true;
        },
        handleChangeWatchCanel:function(){      //点击更改物料弹窗取消按钮
            this.watchNeedle.changerWatchNeedle = false;
        },
        handleChangeWatchSure:function(){        //点击更改物料弹窗确定按钮
            this.watchNeedle.changerWatchNeedle = false;
        },
        // 第四部分（设备信息）
        handleEquNewBuCancel:function(){        //点击新建弹窗取消按钮
            this.equipment.newBuildIsshow = false
        },
        handleEquNewBuSure:function(){
            this.equipment.newBuildIsshow = false;
            // 请求接口成功时
            this.$confirm('设备创建成功，是否需要创建新合约?', '提示', {
                confirmButtonText: '创建',
                cancelButtonText: '关闭',
                type: 'warning'
            }).then(() => {
                this.equipment.contractIsshow = true
                // this.$message({
                //     type: 'success',
                //     message: '成功!'
                // });
            }).catch(() => {
                this.$message({
                    type: 'info',
                    message: '取消创建新合约'
                });          
            });

        }       
      },
      mounted:function(){
        axios.post('/user', {
            firstName: 'Fred',
            lastName: 'Flintstone'
        })
        .then(function (response) {
            console.log(response);
        })
        .catch(function (error) {
            console.log(error);
        });
      }
