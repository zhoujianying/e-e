                                      <el-select v-model="created.city" placeholder="请选择">
                                          <el-option
                                              v-for="item in created.citys"
                                              :key="item.value"
                                              :label="item.label"
                                              :value="item.value">
                                          </el-option>
                                      </el-select>
                                    </div>                   
                                </div>
                            </el-col>
                            <el-col :span="12">
